<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE simulation SYSTEM "simulation.dtd"> 
<simulation finalinst="100" initpop="20" maxpop="650" comfortsens="2">
    <grid colsnb="10" rowsnb="10"/>
    <initialpoint xinitial="1" yinitial="1"/>
    <finalpoint xfinal="9" yfinal="10"/>
    <specialcostzones num="2">
        <zone xinitial="2" yinitial="2" xfinal="3" yfinal="3">4</zone>
        <zone xinitial="4" yinitial="4" xfinal="5" yfinal="5">5</zone>
    </specialcostzones>
    <obstacles num="5">
        <obstacle xpos="2" ypos="1"/>
        <obstacle xpos="2" ypos="3"/>
        <obstacle xpos="2" ypos="4"/>
        <obstacle xpos="4" ypos="2"/>
        <obstacle xpos="6" ypos="8"/>
    </obstacles>
    <events>
        <death param="8"/>
        <reproduction param="1"/>
        <move param="2"/>
    </events>
</simulation>

